{
  "name": "crawler",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/ftp": "^0.3.32",
    "@types/xlsx": "^0.0.36",
    "axios": "^0.21.4",
    "date-fns": "^2.23.0",
    "express": "^4.17.1",
    "ftp": "^0.3.10",
    "shx": "^0.3.3",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "copy-files": "shx cp -r src/files dist/files",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-files",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start"
  }
}
