{
  "name": "crawler",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cron": "^1.7.3",
    "@types/date-fns": "^2.6.0",
    "@types/ftp": "^0.3.32",
    "@types/node-cron": "^3.0.0",
    "@types/xlsx": "^0.0.36",
    "@types/xml2js": "^0.4.9",
    "axios": "^0.21.4",
    "cron": "^1.8.2",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.23.0",
    "ftp": "^0.3.10",
    "iconv-lite": "^0.6.3",
    "shx": "^0.3.3",
    "xlsx": "^0.17.1",
    "xml2js": "^0.4.23",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "copy-files": "shx cp -r src/files dist/files",
    "copy-config": "shx cp src/config.json dist",
    "copy-log": "shx cp src/log.csv dist",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-files copy-config copy-log",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start"
  }
}
